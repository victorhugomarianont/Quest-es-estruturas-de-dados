#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(){   
    int i, j;
    char string[100];
    scanf("%s", string);
    for (i = 0; string[i]; ++i){
        if (string[i] != '(')
            continue;
        for (j = i + 1; string[j]; ++j){
            if (string[j] == ')'){
                string[i] = 'X';
                string[j] = 'X';
                break;
            }
        }
    }
    int acumulador = 0;
    for (i = 0; string[i]; ++i)
        if (string[i] == '(')
            ++acumulador;
    if (acumulador == 0)
        printf("Partiu RU!");
    else
        printf("Ainda temos %d assunto(s) pendente(s)!\n", acumulador);
    return 0;
} 
